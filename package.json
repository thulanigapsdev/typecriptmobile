{
  "name": "expo-starter",
  "version": "3.3.0",
  "description": "Production-ready starter for Expo (React Native) App! Powered by cli-rn, React Navigation (v6), RN UI lib, Mobx, Reanimated 2, Dark Mode, Localization, and much more.",
  "license": "MIT",
  "author": "Batyr <dev@batyr.io>",
  "main": "node_modules/expo/AppEntry.js",
  "scripts": {
    "start": "expo start",
    "start:dc": "expo start --dev-client",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "eject": "expo eject",
    "p:web": "expo build:web && cd web-build && vercel --prod && cd ..",
    "p:app": "expo p",
    "p:all": "yarn p:app && yarn p:web",
    "format": "prettier --check ./src",
    "format:write": "prettier --write ./src",
    "release": "dotenv release-it"
  },
  "dependencies": {
    "@react-native-community/netinfo": "6.0.2",
    "@react-navigation/native": "^6.0.6",
    "expo": "^43.0.0",
    "firebase": "^9.6.1",
    "react": "17.0.1",
    "react-dom": "17.0.1",
    "react-native": "0.64.3",
    "react-native-appearance": "^0.3.4",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@expo/config-plugins": "^4.0.15",
    "@expo/webpack-config": "^0.16.13",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/react": "~17.0.21",
    "@types/react-dom": "~17.0.9",
    "@types/react-native": "~0.64.12",
    "@typescript-eslint/parser": "^5.4.0",
    "dotenv-cli": "^4.1.0",
    "prettier": "^2.4.1",
    "release-it": "^14.11.8",
    "typescript": "~4.3.5"
  }
}
